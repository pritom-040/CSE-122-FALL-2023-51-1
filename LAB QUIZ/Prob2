#include<bits/stdc++.h>
using namespace std;


class show {
public:
    int a,b,c;

    void in() {
        std::cin>>a>>b>>c;
    }

    void out() {
        std::cout<<std::max({a, b, c})<<std::endl;
    }
};

class floatval : public show {
public:
    float d,e,f;

    void in() {
        show::in();
        std::cin>>d>>e>>f;
    }

    void out() {
        show::out();
        std::cout<<std::max({d, e, f})<<std::endl;
    }
};

class charsvis : public floatval {
public:
    char x,y,z;

    void in() {
        floatval::in();
        std::cin>>x>>y>>z;
    }

    void out() {
        floatval::out();
        std::cout<<std::max({x, y, z})<<std::endl;
    }
};

int main() {
    charsvis obj;
    obj.in();
    obj.out();


}
