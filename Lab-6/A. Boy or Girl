#include <stdio.h>
#include <string.h>

int main()
{
    char a[100];
    scanf("%s", a);

    int distinctCount = 0;
    int length = strlen(a);

    for (int i = 0; i < length; i++)
    {
        int isDistinct = 1;

        for (int j = 0; j < i; j++)
        {
            if (a[i] == a[j])
            {
                isDistinct = 0;
                break;
            }
        }

        if (isDistinct)
        {
            distinctCount++;
        }
    }

    if (distinctCount % 2 == 0)
    {
        printf("CHAT WITH HER!");
    }
    else
    {
        printf("IGNORE HIM!");
    }

    return 0;
}
