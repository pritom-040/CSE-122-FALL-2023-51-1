#include<bits/stdc++.h>
using namespace std;
int main()
{
   int n,i;
   cin>>n;
   string a;
   cin>>a;
   int cnt=0,cnt1=0;
   for(i=0;i<a.length();i++){
    if(a[i]=='A'){
        cnt++;
    }
    else{
        cnt1++;
    }
   }
   if(cnt>cnt1){
    cout<<"Anton";
   }
   else if(cnt1>cnt){
    cout<<"Danik";
   }
   else if(cnt==cnt1){
    cout<<"Friendship";
   }
}
